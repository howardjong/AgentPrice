[[workflows.workflow.tasks]]
task = "shell.exec"
args = "  const filePath = path.join(outputDir, file);"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "  if (!fs.existsSync(filePath)) {"

[[workflows.workflow.tasks]]

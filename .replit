args = "  echo \"SCHEDULED HEALTH CHECK: $(date)\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "  echo \"=======================================\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "  node tests/manual/system-health-check.js"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = '  echo "\nWaiting 30 minutes before next check..."'

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "  sleep 1800"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "done"


args = "const sampleData = {"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "  plotlyConfig: {"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "    data: [{x: [1, 2, 3], y: [1, 2, 3], type: 'scatter', name: 'Sample Data'}],"

[[workflows.workflow.tasks]]
